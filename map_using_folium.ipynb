    " # Update folium Map"
    "import folium\n",
    "\n",
    "# Coordinates for the rescue station\n",
    "rescue_station_location = [32.592845, 74.046666]  # Example coordinates for the rescue station\n",
    "\n",
    "# Coordinates for 9 key ambulance locations\n",
    "ambulance_locations = [\n",
    "    [32.562398, 74.079633],\n",
    "    [32.556709, 74.038097],\n",
    "    [32.569043, 74.099731],\n",
    "    [32.537587, 74.062328],\n",
    "    [32.596399, 74.07937],\n",
    "    [32.541738, 74.004267],\n",
    "    [32.587905, 74.076675],\n",
    "    [32.60316, 74.037822],\n",
    "    [32.592845, 74.046666],\n",
    "]\n",
    "\n",
    "# Create a map centered around the rescue station\n",
    "map = folium.Map(location=rescue_station_location, zoom_start=14)\n",
    "\n",
    "# Custom icon for the rescue station\n",
    "station_icon_url = 'images (1).png'  # Replace with the URL or local path of your station icon\n",
    "station_icon = folium.CustomIcon(\n",
    "    icon_image=station_icon_url,\n",
    "    icon_size=(50, 50)\n",
    ")\n",
    "\n",
    "# Add the rescue station marker to the map\n",
    "folium.Marker(\n",
    "    location=rescue_station_location,\n",
    "    icon=station_icon,\n",
    "    popup=\"Rescue Station\"\n",
    ").add_to(map)\n",
    "\n",
    "# Loop to add each ambulance location with its own CustomIcon instance\n",
    "ambulance_icon_url = 'bike.png'  # Replace with the URL or local path of the bike PNG icon\n",
    "\n",
    "for i, location in enumerate(ambulance_locations, start=1):\n",
    "    # Create a new CustomIcon for each ambulance point\n",
    "    ambulance_icon = folium.CustomIcon(\n",
    "        icon_image=ambulance_icon_url,\n",
    "        icon_size=(40, 40)\n",
    "    )\n",
    "    \n",
    "    # Add marker with custom ambulance icon\n",
    "    folium.Marker(\n",
    "        location=location,\n",
    "        icon=ambulance_icon,\n",
    "        popup=f\"Ambulance Location {i}\"\n",
    "    ).add_to(map)\n",
    "\n",
    "# Save the map to an HTML file\n",
    "map.save(\"rescue_station_and_ambulances_map.html\")\n"
